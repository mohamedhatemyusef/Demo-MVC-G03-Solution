@model EmployeeViewModel

<div class="form-group">
    <label asp-for="Name" class="col-form-label"></label>
    <input readonly asp-for="Name" class="form-control" />
</div>

<div class="form-group">
    <label asp-for="Age" class="col-form-label"></label>
    <input readonly asp-for="Age" class="form-control" />
</div>
<div class="form-group">
    <label asp-for="Salary" class="col-form-label"></label>
    <input readonly asp-for="Salary" class="form-control" />
</div>
<div class="form-group">
    <label asp-for="Address" class="col-form-label"></label>
    <input readonly asp-for="Address" class="form-control" />
</div>
<div class="form-group">
    <label asp-for="Phone" class="col-form-label"></label>
    <input readonly asp-for="Phone" class="form-control" />
</div>
<div class="form-group">
    <label asp-for="Email" class="col-form-label"></label>
    <input readonly asp-for="Email" class="form-control" />
</div>
<div class="form-group">
    <input disabled asp-for="IsActive" class="form-check" />
    <label asp-for="IsActive"></label>
</div>

<div class="form-group">
    <input disabled asp-for="IsDeleted" class="form-check" />
    <label asp-for="IsDeleted"></label>
</div>
<div class="form-group">
    <label asp-for="HireDate" class="col-form-label"></label>
    <input readonly asp-for="HireDate" class="form-control" />
</div>

<div class="form-group">
    <label asp-for="DateOfCreate" class="col-form-label"></label>
    <input readonly asp-for="DateOfCreate" class="form-control" />
</div>
